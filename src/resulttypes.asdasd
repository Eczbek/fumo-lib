#include "fumo/fumo_data_structures.h"

Result get_input() {
    int n = 0;
    if (scanf("%d", &n)) {
        return Ok(n);
    }
    return Err("scanf failed bruh.\n");
}

int main() {

    match(get_input())({
        _Ok(int, var) printf("_Ok: %d.\n", *var);

        _Err(char_ptr, errval) {
            printf("error message: %s", *errval);
        }
    });
}
